{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "contentBlocks": {
      "type": "dynamiczone",
      "components": [
        "blog.title",
        "blog.description",
        "blog.image",
        "blog.hr"
      ]
    },
    "subtitle": {
      "type": "string"
    },
    "category": {
      "type": "component",
      "repeatable": true,
      "component": "blog.category"
    },
    "countries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "mappedBy": "blogs"
    }
  }
}
